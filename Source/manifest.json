{
	"manifest_version": 2,
	"name": "ebolatozombie",
	"version": "1.1",
	"description": "Replaces the text 'Ebola' with 'zombie'.",
	"icons": {
	  "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
	},
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

